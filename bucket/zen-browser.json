{
  "version": "1.0.1-a.14",
  "description": "A open-source browser based on the Firefox engine",
  "homepage": "https://www.zen-browser.app/",
  "license": "MPL-2.0",
  "url": "https://github.com/zen-browser/desktop/releases/download/1.0.1-a.14/zen.win-specific.zip",
  "extract_dir": "zen.win-specific\\zen",
  "bin": [
    [
      "zen.exe",
      "zen",
      "-profile \"$dir\\profile\""
    ]
  ],
  "shortcuts": [
      [
        "zen.exe",
        "Zen Browser",
        "-profile \"$dir\\profile\""
      ]
  ],
  "persist": "Profile",
  "checkver": {
    "url": "https://www.zen-browser.app/release-notes",
    "regex": "/release-notes/([ab\\d.-]+)"
  },
  "autoupdate": {
    "url": "https://github.com/zen-browser/desktop/releases/download/$version/zen.win-specific.zip"
  }
}
